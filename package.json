{
  "name": "flutter-component-separator",
  "displayName": "Flutter Component Separator",
  "description": "Separates Flutter components into individual files",
  "version": "0.0.2",
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.separateFlutterComponents"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.separateFlutterComponents",
        "title": "Separate Flutter Components"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.60.0",
    "@types/node": "^14.x",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "eslint": "^8.26.0",
    "typescript": "^4.8.4",
    "@vscode/test-electron": "^2.1.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/abdulmominsakib/flutter-component-separator.git"
  },
  "license": "MIT",
  "publisher": "AbdulMominSakib"
}